    (*i)->accept(this);
  }
}

void TypeChecker::visitListId(ListId* listid)
{
  printf("visitListId\n");
  for (ListId::iterator i = listid->begin() ; i != listid->end() ; ++i)
  {
    visitId(*i) ;
  }
}


